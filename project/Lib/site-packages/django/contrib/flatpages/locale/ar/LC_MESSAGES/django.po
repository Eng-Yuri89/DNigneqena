#: .\project\Lib\site-packages\django\contrib\flatpages\admin.py:12
msgid "Advanced options"
msgstr "خيارات متقدّمة"

#: .\project\Lib\site-packages\django\contrib\flatpages\apps.py:7
msgid "Flat Pages"
msgstr "صفحات مسطحة"

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:10
#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:9
msgid "URL"
msgstr "رابط"

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:13
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr "مثال: “/about/contact/”. تأكد من وضع فواصل مائلة في البداية والنهاية."

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:16
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"يجب أن تحتوي هذه القيمة الأحرف والأرقام والنقاط وعلامات _ و - و / أو ~ فقط."

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:30
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr "مثال: “/about/contact/”. تأكد من وضع فواصل مائلة في البداية."

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:43
msgid "URL is missing a leading slash."
msgstr "العنوان URL يفقد رمز / في بدايته."

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:48
msgid "URL is missing a trailing slash."
msgstr "العنوان URL يفقد رمز / في نهايته."

#: .\project\Lib\site-packages\django\contrib\flatpages\forms.py:65
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "الصفحة ذو العنوان %(url)s موجودة سابقاً في موقع %(site)s"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:10
msgid "title"
msgstr "العنوان"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:11
msgid "content"
msgstr "المحتوى"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:12
msgid "enable comments"
msgstr "السماح بالتعليقات"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:14
msgid "template name"
msgstr "اسم القالب"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"مثال: “flatpages/contact_page.html”. إن لم يكن هذا موجوداً، فسوف يستخدم "
"النظام 'flatpages/default.html'."

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:23
msgid "registration required"
msgstr "التسجيل مطلوب"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"إذا كان هذا الخيار محددا، فإن المستخدمين الداخلين فقط سيتمكنون من مشاهدة "
"الصفحة."

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:27
msgid "sites"
msgstr "المواقع"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:31
msgid "flat page"
msgstr "صفحة مسطحة"

#: .\project\Lib\site-packages\django\contrib\flatpages\models.py:32
msgid "flat pages"
msgstr "صفحات مسطحة"
